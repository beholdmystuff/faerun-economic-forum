  let activeGroups = [];

  function toggleFilter(event) {
    const button = event.target;
    const group = button.textContent.toLowerCase();
    const index = activeGroups.indexOf(group);
    if (index > -1) {
      activeGroups.splice(index, 1);
      button.classList.remove("active");
    } else {
      activeGroups.push(group);
      button.classList.add("active");
    }
    filterItems();
  }

  function renderStars(rating) {
    return "⭐".repeat(rating);
  }

  function renderMarkets(label, list) {
    if (!list || list.length === 0) return "";
    return `<p><b>${label}:</b><br>` +
           list.map(m => `<div style="padding-left:5px;">${m.city} ${renderStars(m.rating)}</div>`).join("") +
           `</p>`;
  }

  function displayItems() {
    const container = document.getElementById("itemContainer");
    container.innerHTML = "";

    for (const group in groupedItems) {
      if (activeGroups.length && !activeGroups.includes(group)) continue;

      const groupWrapper = document.createElement("div");
      groupWrapper.className = "group";
      const groupHeader = document.createElement("h2");
      groupHeader.textContent = group.charAt(0).toUpperCase() + group.slice(1);
      const itemRow = document.createElement("div");
      itemRow.className = "item-row";

      groupedItems[group].forEach(item => {
        const itemDiv = document.createElement("div");
        itemDiv.className = "item";

        const imports = renderMarkets("Imports", item.import_markets);
        const exports = renderMarkets("Exports", item.export_markets);

        itemDiv.innerHTML = `
          <a href="${group}/${item.product}.png">
            <img src="${group}/${item.product}_t.jpg" alt="${item.product}"
              onerror="this.onerror=null;this.src='placeholder.png';this.className='off'">
          </a>
          <h2><a href="${item.url}" target="_blank">${item.product} ⧉</a></h2>
          <p>${item.description}</p>
          ${imports}
          ${exports}
        `;
        itemRow.appendChild(itemDiv);
      });

      groupWrapper.appendChild(groupHeader);
      groupWrapper.appendChild(itemRow);
      container.appendChild(groupWrapper);
    }
  }

  function filterItems() {
    const searchValue = document.getElementById("searchBar").value.toLowerCase();
    const container = document.getElementById("itemContainer");
    container.innerHTML = "";

    for (const group in groupedItems) {
      if (activeGroups.length && !activeGroups.includes(group)) continue;

      const matches = groupedItems[group].filter(item =>
        item.product.toLowerCase().includes(searchValue) ||
        item.description.toLowerCase().includes(searchValue)
      );

      if (!matches.length) continue;

      const groupWrapper = document.createElement("div");
      groupWrapper.className = "group";
      const groupHeader = document.createElement("h2");
      groupHeader.textContent = group.charAt(0).toUpperCase() + group.slice(1);
      const itemRow = document.createElement("div");
      itemRow.className = "item-row";

      matches.forEach(item => {
        const itemDiv = document.createElement("div");
        itemDiv.className = "item";

        const imports = renderMarkets("Imports", item.import_markets);
        const exports = renderMarkets("Exports", item.export_markets);

        itemDiv.innerHTML = `
          <a href="${group}/${item.product}.png">
            <img src="${group}/${item.product}_t.jpg" alt="${item.product}"
              onerror="this.onerror=null;this.src='placeholder.png';this.className='off'">
          </a>
          <h2><a href="${item.url}" target="_blank">${item.product} ⧉</a></h2>
          <p>${item.description}</p>
          ${imports}
          ${exports}
        `;
        itemRow.appendChild(itemDiv);
      });

      groupWrapper.appendChild(groupHeader);
      groupWrapper.appendChild(itemRow);
      container.appendChild(groupWrapper);
    }
  }

  displayItems();
</script>

</body>
</html>

